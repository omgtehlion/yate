/// {
///     description: 'ternary operator',
///     data: {
///         foo: true,
///         item: [
///             { id: 1, title: 'First' },
///             { id: 2, title: 'Second' },
///             { id: 3, title: 'Third' }
///         ]
///     },
///     result: 'fooSecond'
/// }

match / {
    .foo ? "foo" : "bar"

    .item[ /.foo ? 1 : 2 ].title
}

